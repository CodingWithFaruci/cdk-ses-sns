{
    "name": "cdk-ses-sns",
    "version": "0.1.0",
    "bin": {
        "cdk-ses-sns": "bin/cdk-ses-sns.js"
    },
    "scripts": {
        "lint": "npx eslint **/*.ts --fix --quiet",
        "build": "npx rimraf dist & tsc",
        "build-w": "npx rimraf dist & webpack",
        "watch": "tsc -w",
        "test": "jest --passWithNoTests",
        "diff-dev": "npx cdk diff cdk-ses-sns-dev --ci --require-approval never --no-asset-metadata",
        "diff-tst": "npx cdk diff cdk-ses-sns-tst --ci --require-approval never --no-asset-metadata",
        "diff-prd": "npx cdk diff cdk-ses-sns-prd --ci --require-approval never --no-asset-metadata",
        "deploy-dev": "npx cdk deploy cdk-ses-sns-dev --ci --require-approval never --no-asset-metadata",
        "deploy-tst": "npx cdk deploy cdk-ses-sns-tst --ci --require-approval never --no-asset-metadata",
        "deploy-prd": "npx cdk deploy cdk-ses-sns-prd --ci --require-approval never --no-asset-metadata"
    },
    "devDependencies": {
        "@types/jest": "^28.1.0",
        "@types/node": "^17.0.38",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "aws-cdk": "2.26.0",
        "eslint": "^8.16.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-jest": "^26.4.6",
        "jest": "^28.1.0",
        "jest-sonar": "^0.2.12",
        "ts-jest": "^28.0.3",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.0",
        "typescript": "~4.7.2",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "aws-cdk-lib": "2.26.0",
        "constructs": "^10.1.25",
        "source-map-support": "^0.5.21"
    }
}
